
{
    "manifest_version": 2,
    "name": "Facebook Troll",
    "version": "1.0.0",
    "description": "",
  
    "content_scripts": [{
      "js": ["content.js"],
      "matches": ["http://www.facebook.com/*", "https://www.facebook.com/*"]
    }],
    "permissions": [
        "tabs", "http://www.facebook.com/*", "https://www.facebook.com/*","activeTab"
      ]
  
  }